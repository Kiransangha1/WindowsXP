@{
    ViewData["Title"] = "desktop";
    Layout = "_Empty";
}

<style>
    body {
        background-image: url('https://images.nrc.nl/wo6i1IGemSy9WfvMRKxm0w0C02E=/1280x/filters:no_upscale()/s3/static.nrc.nl/tech/files/2014/04/HD-Windows-XP-Bliss-Wallpaper-Backgrounds.jpg');
        background-size: 100% 100%;
        background-repeat: no-repeat;
        background-position: center;
    }

    .framing {
        display: flex;
        flex-wrap: wrap;
    }

    .blue-bar {
        position: absolute;
        bottom: 0;
        left: 0;
        width: 100%;
        height: 40px;
        background-color: #225cd9;
        display: flex;
        flex-wrap: wrap;
    }

    .green-bar {
        background-color: green;
        width: 150px;
        border-radius: 0 15px 15px 0;
        cursor: pointer;
    }

    .date {
        margin-left: 90%;
    }

    .shutdown {
        background-image: url('https://64.media.tumblr.com/4e932aabdfe572f340dbe32cfc06f1cb/840b4b6888f1f93f-e3/s540x810/c3d700cbea3465ba7c8215250e988acb638676e8.png');
        image: url('https://64.media.tumblr.com/4e932aabdfe572f340dbe32cfc06f1cb/840b4b6888f1f93f-e3/s540x810/c3d700cbea3465ba7c8215250e988acb638676e8.png');
        margin-left: 500px;
    }

    .start {
        background-image: url('https://cdn2.steamgriddb.com/icon_thumb/e07413354875be01a996dc560274708e.png');
        background-repeat: no-repeat;
        background-size: contain;
        width: 40px;
        height: 40px;
        margin-left: 10px;
        cursor: pointer;
    }

    .text {
        color: white;
        margin-left: 50px;
        cursor: pointer;
    }

    .icon {
        width: 90px;
        height: 90px;
        margin-right: 50px;
        margin-bottom: 65px;
        margin-top: 10px;
    }

    .icon img {
        width: 100%;
        height: 100%;
        border-radius: 10px;
    }

    .icon-text {
        color: white;
        text-shadow: 0px 0px 2px black, 0px 0px 4px black;
    }

    .float {
        float: right;
    }

    .image-display {
        display: none;
        position: absolute;
        top: -478px;
        left: 13%;
        transform: translateX(-50%);
        z-index: 1;
    }

    .checkbox {
        display: none;
    }

    .checkbox:checked+.image-display {
        display: block;
    }

    .green-bar-label {
        cursor: pointer;
    }

    .green-bar-label:active+.checkbox+.image-display,
    .start-label:active+.checkbox+.image-display {
        display: block;
    }
</style>


<div class="blue-bar">
    <div class="green-bar">
        <label class="green-bar-label start" for="checkbox">
            <h2 class="text">start</h2>
        </label>
        <div>
            <label class="checkbox" for="checkbox"></label>
        </div>
    </div>
        <div style="display: flex;">
        @{var currentDateTime = DateTime.Now; }

        <p class="float">@currentDateTime.ToString("hh:mm tt")</p> 
    </div>
    <input class="checkbox" id="checkbox" type="checkbox">
    <img class="image-display"
        src="https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/1bafc2fa-cc31-4e36-b718-4246d87d2cf2/dek88r9-2fcd78eb-45d8-429f-8033-2f208ccdac49.png/v1/fill/w_380,h_478,q_80,strp/windows_xp_start_menu_template_by_user12720127301998_dek88r9-fullview.jpg?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..SeAfR2yFxxr8zXUP0YiywnafRTxYfmqejS4sd5NqNUM"
        alt="Windows XP Start Menu" />
</div> 

