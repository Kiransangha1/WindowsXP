@{
    ViewData["Title"] = "turnoff?";
    Layout = "_Empty";
}

<style>
    body {
        background-image: url('https://thetechgears.com/wp-content/uploads/2014/04/Microsoft-Ends-Support-for-Windows-XP.jpg');
        background-size: 100% 100%;
        background-repeat: no-repeat;
        background-position: center;
    }
</style>

<a asp-action="ShuttingDown" asp-controller="Computer">
